---
<script>
    
	var splide = new Splide(document.getElementById('partner'), {
	    type   : 'loop',
        perPage: 6,
        gap:4,
        pagination:false,
        arrows:false,
        autoScroll: {
            speed: 1,
        },
        breakpoints: {
            640: {
            perPage: 3,
            },
            480: {
            perPage: 2,
            },
        }
	});
	splide.mount(window.splide.Extensions);

</script>

<div class="flex justify-center items-center px-10 md:flex-row flex-col">
    <div class="mr-3">
        <h1 class="text-center text-blueColor font-light uppercase lg:text-2xl text-xl mb-0 ">Our</h1>
        <h1 class="text-center text-blueColor font-bold lg:text-3xl text-2xl uppercase">partners</h1>
    </div>
 
    <div>

        <section class="splide mt-10" aria-labelledby="carousel-heading" id="partner">
            <div class="splide__track">
                <ul class="splide__list">
                    <li class="splide__slide">
                        <div class="flex justify-center items-center img-con">

                            <img class="max-w-full max-h-full" src={Emer}/>
                        </div>
                    </li>
                    <li class="splide__slide">
